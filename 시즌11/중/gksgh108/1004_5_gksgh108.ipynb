N = int(input())
tteok = list(map(int, input().split()))

# 정렬
tteok.sort()

# 최대 중복 개수 찾기
max_count = current_count = 1

for i in range(1, N):
    if tteok[i] == tteok[i - 1]:
        current_count += 1
    else:
        max_count = max(max_count, current_count)
        current_count = 1

max_count = max(max_count, current_count)  # 마지막 그룹 체크

print(max_count)
